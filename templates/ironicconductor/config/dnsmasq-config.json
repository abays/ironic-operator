{
    "command": "/usr/sbin/dnsmasq -k",
    "config_files": [
        {
            "source": "/var/lib/ironic/dnsmasq.conf",
            "dest": "/etc/dnsmasq.conf",
            "owner": "dnsmasq",
            "perm": "0644"
        }
    ],
    "permissions": [
        {
            "path": "/var/lib/ironic",
            "owner": "ironic:ironic",
            "recurse": true
        }
    ]
}
